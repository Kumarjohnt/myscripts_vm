location  / {
                fastcgi_read_timeout 300s;
        	proxy_set_header   Host             $host;

    if ( $request_come_from = "INTERNAL" ) {
         proxy_pass https://srcx-prod.vmware.com;
         break;
 }

  if ( $http_cookie ~* "aff6460ec7" ) {
                 proxy_pass https://srcx-prod.vmware.com;
                  break;
       }

set $pm 0;
    if ( $request_come_from = "EXTERNAL" ) {
          set $pm  "1";
       }
    if ( $http_cookie ~* "source_token" ) {
               set $pm "${pm}1";
       }

      if ( $pm = "11" ){
           rewrite .*  https://source.vmware.com/intranet/auth/validate-token/?token=$cookie_source_token&sharepoint_url=$request_uri redirect;
      }


     if ( $pm != "11" ) {
          rewrite .*  https://source.vmware.com/?sharepoint_url=$request_uri redirect;

}

}


